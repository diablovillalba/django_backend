{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "albumId",
      "id",
      "title",
      "url",
      "thumbnailUrl"
    ],
    "properties": {
      "albumId": {
        "$id": "#/items/properties/albumId",
        "type": "integer",
        "title": "The Albumid Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "title": {
        "$id": "#/items/properties/title",
        "type": "string",
        "title": "The Title Schema",
        "default": "",
        "examples": [
          "accusamus beatae ad facilis cum similique qui sunt"
        ],
        "pattern": "^(.*)$"
      },
      "url": {
        "$id": "#/items/properties/url",
        "type": "string",
        "title": "The Url Schema",
        "default": "",
        "examples": [
          "https://via.placeholder.com/600/92c952"
        ],
        "pattern": "^(.*)$"
      },
      "thumbnailUrl": {
        "$id": "#/items/properties/thumbnailUrl",
        "type": "string",
        "title": "The Thumbnailurl Schema",
        "default": "",
        "examples": [
          "https://via.placeholder.com/150/92c952"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}